<% include ./partials/header %>

  <div class="container text-center">
    <div class="row">
      <div class="col">
        <div class="jumbotron">
          <h1 class="display-4 rs-name">Request <span id="vegan">Rooter's</span> at your event.</h1>
          <p class="lead">Submit a request and we will be in touch ASAP!</p>
          <hr>
          <form class="contact">
            <div class="form-group">
              <input class="form-control" type="text" name="" placeholder="name" />
            </div>
            <div class="form-group">
              <input class="form-control" type="text" name="" placeholder="email" />
            </div>
            <div class="form-group">
              <input class="form-control" type="date" name="" placeholder="date" />
            </div>
            <div class="form-group">
              <textarea class="form-control" placeholder="Briefly Describe your event and needs and we will be in touch as soon as possible."></textarea>
            </div>
            <div class="form-group">
              <input id="contactbtn" class=" form-control btn  btn-lg  btn-block" value="Submit" />
            </div>

          </form>
        </div>
      </div>
    </div>
  </div>

  <% include ./partials/footer %>
